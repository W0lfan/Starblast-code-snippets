var ship_of_the_game = [Rock_Tower_791,Rock_Tower_801, Shadow_X_2_792, Shadow_X_2_802 ];


this.options = {
  // see documentation for options reference
  root_mode: "survival",
  map_size: 30,
  ships: ship_of_the_game,
  survival_level: 10,
  crystal_value: 50,
  starting_ship: 701,
};


var tier_791 = {
  id: "tier_791",
  position: [35,0,15,15],
  clickable: true,
  shortcut: "9",
  visible: true,
  components: [
    { type: "box",position:[0,0,100,100],stroke:"#CDE",width:2},
    { type: "text",position:[10,10,80,30],value:"Tier 8",color:"#CDE"},
    { type: "text",position:[10,35,80,30],value:"OP ROCK TOWER",color:"#CDE"},
    { type: "text",position:[20,70,60,20],value:"[9]",color:"#CDE"}
    ]
};


var tier_792 = {
  id: "tier_792",
  position: [50,0,15,15],
  clickable: true,
  shortcut: "0",
  visible: true,
  components: [
    { type: "box",position:[0,0,100,100],stroke:"#CDE",width:2},
    { type: "text",position:[10,10,80,30],value:"Tier 8",color:"#CDE"},
    { type: "text",position:[10,35,80,30],value:"SUPER MEGA SHIP",color:"#CDE"},
    { type: "text",position:[20,70,60,20],value:"[0]",color:"#CDE"}
    ]
};

var addT81 = function(ship) {
  ship.set({type: 791, crystals: 0});
};
var addT82 = function(ship) {
  ship.set({type: 792, crystals: 0});
};

this.tick = function(game) {
  if (game.step % 15 == 0) {
    for (let ship of game.ships) {
      if (ship.type == 701 || 702 || 703 || 704 && ship.crystals == 980) {
          ship.setUIComponent(tier_791);
          ship.setUIComponent(tier_792);
      } 
      if (ship.crystals !== 980) {
          ship.setUIComponent({id: "tier_791", visible: false});
          ship.setUIComponent({id: "tier_792", visible: false});
        }
      }
  }
};



this.event = function(event, game) {
  let ship = event.ship;
  switch (event.name) {
    case "ui_component_clicked":
      switch (event.id) {
        case "tier_791":
          addT81(ship);
          break;
        case "tier_792":
          addT82(ship);
          break;
      }
      break;
    case "ship_destroyed":
      if (event.ship != null)
      {
        shipJustSpawned(ship) ;
      }
      break ;
  }
};
